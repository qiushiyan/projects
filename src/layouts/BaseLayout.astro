---
import MainHead from "@components/MainHead.astro";
import Nav from "@components/Nav.astro";
import "../styles/global.css";

interface Props {
  title?: string;
  description?: string;
}
const props = Astro.props;
---

<html lang="en">
  <MainHead {...props} />
  <body class="bg-slate-900">
    <Nav />
    <main>
      <slot />
    </main>
  </body>
</html>
