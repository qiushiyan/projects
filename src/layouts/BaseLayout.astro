---
import MainHead from "@components/MainHead.astro";
import Nav from "@components/Nav.astro";

interface Props {
  title?: string;
  description?: string;
}
const props = Astro.props
---

<html lang="en">
    <head>
        <MainHead {...props} />
    </head>
    <body class="bg-slate-900">
        <Nav />
        <main>
            <slot />
        </main>
    </body>
</html>


